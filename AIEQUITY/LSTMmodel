        "data = yf.download(\n",
        "    tickers,\n",
        "    interval=\"1d\",\n",
        "    auto_adjust=True,\n",
        "    group_by=\"ticker\",\n",
        "    progress=False\n",
        ")\n",
        "\n",
        "print(data.head())\n"
